{
  "name": "crm-server",
  "version": "0.1.0",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "node ./dist/index.js",
    "start:dev": "node --inspect=5858 -r dotenv/config -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon",
    "migrate": "node -r dotenv/config node_modules/.bin/sequelize db:migrate",
    "migrate:undo": "node -r dotenv/config node_modules/.bin/sequelize db:migrate:undo",
    "migrate:undo:all": "node -r dotenv/config node_modules/.bin/sequelize db:migrate:undo:all",
    "prepare-db": "node -r dotenv/config node_modules/.bin/sequelize db:seed:all",
    "test": "NODE_PATH=./ NODE_ENV=test node_modules/.bin/mocha 'src/**/*.spec.ts'",
    "test:coverage": "nyc npm run test",
    "test:architecture": "NODE_PATH=./ NODE_ENV=test node_modules/.bin/mocha 'tests/architecture/architecture.spec.ts' --timeout 10000"
  },
  "engines": {
    "node": "14.x.x"
  },
  "heroku-run-build-script": true,
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "1.4.5",
    "cors": "*",
    "email-templates": "8.0.3",
    "express": "4.17.1",
    "http-status-codes": "2.1.4",
    "jimp": "0.16.1",
    "jsonwebtoken": "8.5.1",
    "multer": "1.4.2",
    "neverthrow": "4.1.1",
    "nodemailer": "6.5.0",
    "passport": "*",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.0",
    "pg": "8.5.1",
    "pg-hstore": "2.3.3",
    "pino-http": "5.5.0",
    "pino-pretty": "4.6.0",
    "reflect-metadata": "0.1.13",
    "sequelize": "6.5.0",
    "socket.io": "3.1.2",
    "swagger-ui-express": "4.1.6",
    "typedi": "0.10.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@types/chai": "4.2.15",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.10",
    "@types/dotenv": "6.1.1",
    "@types/email-templates": "8.0.2",
    "@types/express": "4.17.11",
    "@types/jsonwebtoken": "8.5.0",
    "@types/mocha": "8.2.1",
    "@types/multer": "1.4.5",
    "@types/node": "14.14.31",
    "@types/passport": "1.0.6",
    "@types/passport-http": "0.3.8",
    "@types/passport-jwt": "3.0.5",
    "@types/pino-http": "5.4.0",
    "@types/sinon": "9.0.10",
    "@types/swagger-ui-express": "4.1.2",
    "@types/validator": "13.1.3",
    "chai": "4.3.3",
    "dotenv": "8.2.0",
    "mocha": "8.3.0",
    "nodemon": "2.0.7",
    "nyc": "15.1.0",
    "prettier": "2.2.1",
    "sequelize-cli": "6.2.0",
    "sinon": "9.2.4",
    "source-map-support": "0.5.19",
    "ts-node": "9.1.1",
    "tsarch": "5.2.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.1.5"
  },
  "nodemonConfig": {
    "watch": [
      "./src"
    ],
    "exec": "npm run start:dev",
    "ext": "ts"
  }
}
