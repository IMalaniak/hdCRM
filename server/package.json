  "engines": {
    "node": "14.x.x"
  },
  "heroku-run-build-script": true,
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@overnightjs/core": "^1.6.9",
    "@overnightjs/logger": "^1.1.9",
    "cookie-parser": "^1.4.5",
    "cors": "*",
    "dotenv": "^8.0.0",
    "email-templates": "^7.1.0",
    "express": "^4.17.1",
    "http-status-codes": "^1.3.2",
    "jimp": "^0.16.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.1",
    "nodemailer": "^6.2.1",
    "passport": "*",
    "passport-jwt": "^4.0.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.13.0",
    "socket.io": "^2.3.0",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.32",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.7",
    "@types/dotenv": "6.1.1",
    "@types/email-templates": "7.1.0",
    "@types/express": "4.17.8",
    "@types/jsonwebtoken": "8.5.0",
    "@types/multer": "1.4.4",
    "@types/node": "12.12.25",
    "@types/passport": "1.0.4",
    "@types/passport-jwt": "3.0.3",
    "@types/sequelize": "4.28.9",
    "@types/socket.io": "2.1.4",
    "@types/validator": "10.11.3",
    "nodemon": "2.0.4",
    "prettier": "2.1.1",
    "sequelize-cli": "5.5.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.0.2"
  },
  "nodemonConfig": {
    "watch": [
      "./src"
    ],
    "exec": "npm run start:dev",
    "ext": "ts"
