{
  "name": "crm-server",
  "version": "0.1.0",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "start:dev": "node --inspect=5858 -r dotenv/config -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon",
    "migrate-db": "node_modules/.bin/sequelize db:migrate",
    "prepare-db": "sequelize db:seed:all --env='production'"
